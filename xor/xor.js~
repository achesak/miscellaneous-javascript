Boolean.xor = function() {
    var r = false;
    for (var i = 0; i < arguments.length; i++) {
   //     if(arguments[i] || r) {
   //         if(arguments[i] && r) {
   //             r = false;
   //         } else {
   //             r = true;
   //         }
   //     } else {
   //         r = false;
   //     }
       r = (arguments || r) ? ((arguments[i] && r) ? false : true) : false;
    }
    return r;
};

